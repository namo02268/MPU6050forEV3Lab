<?xml version="1.0" encoding="utf-8"?>
<EditorDefinitions>
  <PolyGroups ModuleName="MPU6050" ModuleVersion="1.0.0">
    <PolyGroup Name="MPU6050" BlockFamily="Sensor">
      <Parameter Name="Port" CompilerDirectives="OneInputPort" Direction="Input" DefaultValue="1.4" />
			<Parameter Name="AccX" Direction="Output" DataType="Single" Identification="Identification_Acceleration_X.png" />
			<Parameter Name="AccY" Direction="Output" DataType="Single" Identification="Identification_Acceleration_Y.png" />
			<Parameter Name="AccZ" Direction="Output" DataType="Single" Identification="Identification_Acceleration_Z.png" />
      <Parameter Name="Tmp" Direction="Output" DataType="Single" Identification="Identification_Temperature.png" />
      <Parameter Name="GyroX" Direction="Output" DataType="Single" Identification="Identification_Gyro_X.png" />
      <Parameter Name="GyroY" Direction="Output" DataType="Single" Identification="Identification_Gyro_Y.png" />
      <Parameter Name="GyroZ" Direction="Output" DataType="Single" Identification="Identification_Gyro_Z.png" />
      <Parameter Name="TiltX" Direction="Output" DataType="Single" Identification="Identification_Tilt_X.png" />
      <Parameter Name="TiltY" Direction="Output" DataType="Single" Identification="Identification_Tilt_Y.png" />
      <Parameter Name="Threshold" Identification="Identification_Threshold_MPU6050.xml" DataType="Single" Direction="Input" DefaultValue="128" MinValue="0" MaxValue="255" />
      <Parameter Name="Result" CompilerDirectives="Result" Identification="Identification_Result.png" DataType="Boolean" Direction="Output"/>
      <Parameter Name="Comparison" Direction="Input" Configuration="Identification_ComparisonType.xml" Identification="Identification_ComparisonType.xml" DataType="Int32" DefaultValue="4" />

      <Hardware>
				<NXTPlotColor>#ffffab34</NXTPlotColor>
				<EV3PlotColor>#ffd46800</EV3PlotColor>
				<EV3AutoID>-1</EV3AutoID>
				<Direction>Input</Direction>
				<DefaultPort>1.4</DefaultPort>
			</Hardware>

      <Block>
        <Mode>MeasureAcceleration</Mode>
        <Reference Type="VILib" Name="MPU6050MeasureAcceleration.vix" />
        <ParameterReference Name="Port" />
        <ParameterReference Name="AccX" />
        <ParameterReference Name="AccY" />
        <ParameterReference Name="AccZ" />
        <PaletteInfo Weight="0.5" />
        <BlockInterface>Measure</BlockInterface>
        <Flags>PBROnly</Flags>
        <Hardware></Hardware>
      </Block>
      <Block>
        <Mode>MeasureGyroscope</Mode>
        <Reference Type="VILib" Name="MPU6050MeasureGyroscope.vix" />
        <ParameterReference Name="Port" />
        <ParameterReference Name="GyroX" />
        <ParameterReference Name="GyroY" />
        <ParameterReference Name="GyroZ" />
