        <Reference Type="VILib" Name="MPU6050MeasureTilt.vix" />
        <ParameterReference Name="Port" />
        <ParameterReference Name="TiltX" />
        <ParameterReference Name="TiltY" />
        <BlockInterface>Measure</BlockInterface>
        <Flags>PBROnly</Flags>
        <Hardware></Hardware>
